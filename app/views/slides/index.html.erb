<h1>All slides</h1>
<div class="container">
  <div class="row">
    <div class="">
    <% @slides.each do |slide| %>
    <p><%= slide.title %></p>
     <iframe frameborder= "0"  height= "500"  scrolling= "no"  src= "<%= slide.slide.url %>"  style="width: 50%; height: 500px; display:inline-block "  scrolling="yes" webkitallowfullscreen mozallowfullscreen allowfullscreen frameborder="no" >
    <% end %>
  </div>
</div>
